{
    "name": "thereddot/monolog-extra-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle with extra processors and logger to log request/response",
    "keywords": ["log", "logging"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
          "TheRedDot\\MonologExtraBundle\\": "src"
        }
    },
    "authors": [
        {
            "name": "Floran Brutel",
            "email": "bonjour@floran.fr"
        }
    ],
    "require": {
        "php": "^7.1.0",
        "symfony/security": "^3.4 || ^4.0",
        "symfony/config":"^3.4 || ^4.0",
        "symfony/http-foundation": "^3.4 || ^4.0",
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/http-kernel": "^3.4 || ^4.0",
        "symfony/monolog-bundle": "^2.8 || ^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpspec/phpspec": "^5.1",
        "phpstan/phpstan": "^0.11.7"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    },
    "replace": {
        "hexanet/monolog-extra-bundle": "self.version"
    }
}
